{
	"name": "Genderizer",
	"version": "1.0",
	"description": "Highlighting words since 2020",
    "content_scripts": [{
        "matches": ["*://www.gutenberg.org/*"],
        "js": ["content_script.js"],
        "run_at": "document_idle",
        "all_frames": true
    }],
    "manifest_version": 2,
    "web_accessible_resources": [
        "lexicon/german_dictionary.json",
	"lexicon/big_german_dictionary.json",
        "lexicon/spanish_dictionary.json"
    ]
}
